
<div class="navbar navbar-expand-sm navbar-light navbar-tumenu">
  <div class="container-fluid" id="container_nav">
     <%= image_tag "https://res.cloudinary.com/dwalfnpav/image/upload/v1724753047/LogoTuMenu_eg6gkz.png", alt: "TuMenu_logo", id: "logo" %>
    <% unless current_user %>
      <%= link_to "Sign up", new_user_registration_path, class: "btn btn-success", type: "button", id: "sign_up_button" %>
    <% else %>
      <div id="asparagus" data-controller="toggle">
      <i class="bi bi-list" data-action="click->toggle#fire2"></i>
        <div id="profile_options" class="d-none" data-toggle-target="togglableElemento">
          <%= link_to "Create a Post", new_meal_path%>
          <%= link_to "Log Out", destroy_user_session_path, data: {turbo_method: :delete}%>
        </div>
      <%= link_to image_tag("tiger.avif", :class => "avatar") ,profile_path(current_user)%>
      </div>
    <% end %>
  </div>
</div>
